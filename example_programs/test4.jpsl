    tex texture2 = texture2.jpg _out
    tex texture = texture.jpg
}

__main
{
    var uvOffset = [-0.01,0]

    var filterA = [1,0,0]
    var filterB = [0,1,1]

    --b
    uv = uv + uvOffset
    var colA = sample(texture,uv)
    uv = uv - uvOffset

    var colB = sample(texture,uv)

    colA = colA * filterA
    colB = colB * filterB

    var col = [0,0,0]

    col = col + colA
    col = col + colB

    return
}

